{
    "name": "saritasa/dingo-api-custom",
    "type": "library",
    "description": "Saritasa customizations for Dingo/Api library",
    "keywords": ["laravel", "api", "dingo", "restful"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Populov",
            "email": "sergey@saritasa.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "api-ecosystem-for-laravel/dingo-api": "^4.2",
        "illuminate/http": ">=5.5 <12.0",
        "laravel/framework": ">=5.5 <12.0",
        "saritasa/transformers": "1.2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4" : {
            "Saritasa\\DingoApi\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Saritasa\\DingoApi\\SaritasaDingoApiServiceProvider"
            ]
        }
    }
}
