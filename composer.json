{
    "name": "saritasa/dingo-api-custom",
    "type": "library",
    "description": "Saritasa customizations for Dingo/Api library",
    "keywords": ["laravel", "api", "dingo", "restful"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Populov",
            "email": "sergey@saritasa.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "dingo/api": "2.0.*@beta",
        "illuminate/http": "^5.5",
        "laravel/framework": "^5.5",
        "saritasa/transformers": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4" : {
            "Saritasa\\DingoApi\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Saritasa\\DingoApi\\SaritasaDingoApiServiceProvider"
            ]
        }
    }
}
